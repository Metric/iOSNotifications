package iosnotifications;

import com.vantagetechnic.iosnotifications.R;

import android.content.Context;
import android.util.AttributeSet;
import android.view.LayoutInflater;
import android.widget.FrameLayout;

public class LiveView extends FrameLayout {
    public LiveView(Context context, AttributeSet attrs) {
        this(context, attrs, 0);
    }

    public LiveView(Context context, AttributeSet attrs, int style) {
        super(context, attrs, style);
        LayoutInflater.from(context).inflate(R.layout.liveview, this);
    }

	public LiveView(Context context) {
		this(context, null, 0);
	}
}
